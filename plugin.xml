<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
xmlns:android="http://schemas.android.com/apk/res/android"
           id="plugin-tip-tabla-periodica"
      version="1.0.0">

    <name>plugin-tip-tabla-periodica</name>
    <description>Cordova plugin to add gradle</description>
    <license>MIT</license>
    <keywords>cordova</keywords>
    <repo>https://github.com/HeosS/plugin-tip-tabla-periodica</repo>
    <issue>https://github.com/HeosS/plugin-tip-tabla-periodica/issues</issue>

    <engines>
        <engine name="cordova" version=">=3.5.0"/>
        <engine name="cordova-android" version="<9.0.0"/>
    </engines>

    <platform name="android">
        <framework src="build.gradle" custom="true" type="gradleReference" />
    </platform>

</plugin>